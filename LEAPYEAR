year = int(input("which year you want to check? "))
if year%4 == 0:
    if year%100 == 0:
        if year%400 == 0:
            print("the mentioned year is a leap year")
        else:
            print("the mentioned year is not a leap year")
    else:
        print("the mentioned year is  a leap year")

else:
    print("the mentioned year is not a leap year")
